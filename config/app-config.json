{
    "debugMode": false,
    "webapp": {
        "port": "3090",
        "hostPort": "localhost:3090",
        "hostUrl": "https://localhost:3090",
        "folderPath": "/pages/",
        "loginStart": "pages/loginstart.html",
        "loginRedirect": "pages/logincomplete.html",
        "mainPage": "pages/index.html",
        "splashPage": "pages/splashpage.html",
        "splashPageTimeout": 2,
        "firstWidth": 1000,
        "firstHeight": 800,
        "persistentUser": true
    },
    "webFolders": [
        "pages",
        "scripts",
        "lib",
        "node_modules/firebase",
        "node_modules/firebaseui/dist"
    ],
    "apis": {
        "firebaseconfig": "/api/firebaseconfig",
        "logintoken": "/api/logintoken",
        "loginready": "/api/loginready"
    },
    "logout": {
        "google.com": "https://accounts.google.com/Logout",
        "facebook.com": "https://www.facebook.com/logout.php",
        "twitter.com": "https://twitter.com/logout",
        "github.com": "https://github.com/logout"
    },
    "providers": [
        "google.com",
        "github.com",
        "twitter.com",
        "facebook.com",
        "password",
        "phone"
    ]
}